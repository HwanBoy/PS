import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        String a = sc.nextLine();
        String[] listA = a.split(" ");
        int answer = 0;
        if(listA.length > 0){
            answer = listA.length;
            if(listA[0].length() ==0){
                answer = answer -1;
            }
        }
        System.out.println(answer);
    }
}